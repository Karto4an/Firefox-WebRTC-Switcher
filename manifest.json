{
    "manifest_version": 1,
    "name": "Firefox WebRTC Switcher",
    "version": "1.0",
  
    "description": "Toggle WebRTC with a single click.",
    "homepage_url": "https://github.com/Karto4an/Firefox-WebRTC-Switcher",
    "icons": {
      "48": "icons/48.png"
    },
  
    "permissions": ["browserSettings"],
  
    "browser_action": {
      "default_icon": "icons/32.png",
      "default_title": "WebRTC",
      "default_popup": "popup/switcher.html"
    }
}
  